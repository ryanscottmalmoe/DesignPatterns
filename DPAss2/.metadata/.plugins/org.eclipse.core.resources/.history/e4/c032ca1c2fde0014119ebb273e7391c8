
public class BadGuy implements Observer, DisplayElement{

	EyeOfSauron eye;
	String name;
	public BadGuy(EyeOfSauron data, String name)
	{
		this.eye = data;
		this.name = name;
	}
	public void update(int h, int e, int d, int m)
	{
		display();
	}
	public defeated()
	{
		
	}
	
	public void display()
	{
		System.out.println("Current guys spotted: ");
	}
}
