import java.util.*;

public class BadGuy implements Observer, DisplayElement{

	EyeOfSauron eye;
	String name;
	public BadGuy(EyeOfSauron eye, String name)
	{
		this.eye = eye;
		this.name = name;
	}
	public void update(int h, int e, int d, int m)
	{
		display( h, e, d, m);
	}
	public void defeated()
	{
		
	}
	
	public void display()
	{
		System.out.println("Current guys spotted: ");
	}
}
